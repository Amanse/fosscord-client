Global object client

client.accounts
.login()
.register({})
.current()
.cache


useCache(obj) // either object with .cache or direct cache object

useCache rerenders if any object in the cache changes

Cache = Collection

function renderMessages({channel_id}) {
	useCache(client.channels.resolve(channel_id).messages)
}
